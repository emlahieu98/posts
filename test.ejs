<ul class="pagination justify-content-center">
  <% if (page > 1) { %>
  <li class="page-item">
    <a class="page-link" href="/launch/?page=<%= page - 1 %>" aria-label="Previous">
      <span aria-hidden="true">&laquo;</span>
      <span class="sr-only">Previous</span>
    </a>
  </li>
  <% } %>
  <% range.map((i) => { %>
  <li class="page-item <%= page === i ? "active" : "" %>"></li>
  <% if (i === "...") { %>
  <span> <%= i %> </span>
  <% } else { %>
  <a class="page-link" href="/launch/?page=<%= i %>"><%= i %></a>
  <% } %>
  </li>
  <% }) %>
  <% if (page < totalPages) { %>
  <li class="page-item ">
    <a class="page-link" href="/launch/?page=<%= page + 1 %>"> Next </a>
  </li>
  <% } %>
</ul>
